{
  "name": "pg6301-innlevering-andersholt",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "dev": "concurrently npm:dev:server npm:dev:client",
    "dev:client": "cd client && npm run dev",
    "dev:server": "cd server && npm run dev",
    "build": "npm run build:server && npm run build:client",
    "build:client": "cd client && npm ci --include=dev && npm run build",
    "build:server": "cd server && npm ci",
    "format": "prettier --write **/*.{js,jsx}",
    "format:check": "prettier --check **/*.{js,jsx}",
    "test": "jest",
    "prepare": "husky install",
    "verify": "jest -- --collectCoverage"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "jest": "^27.4.7",
    "concurrently": "^7.0.0",
    "husky": "^7.0.4",
    "prettier": "^2.5.1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kristiania-pg6301-2022/pg6301-innlevering-andersholt.git"
  },
  "engines": {
    "node": "16.13.2",
    "npm": "8.3.1"
  },
  "bugs": {
    "url": "https://github.com/kristiania-pg6301-2022/pg6301-innlevering-andersholt/issues"
  },
  "homepage": "https://github.com/kristiania-pg6301-2022/pg6301-innlevering-andersholt#readme",
  "babel": {
    "presets": [
      "@babel/preset-env",
      "@babel/preset-react"
    ]
  },
  "jest": {
    "collectCoverage": true,
    "testEnvironment": "jsdom",
    "collectCoverageFrom": [
      "**/*.{js,jsx}"
    ]
  }
}
